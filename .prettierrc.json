{
    "singleQuote": true,
    "semi": false,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "endOfLine": "auto",
    "trailingComma": "all",
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.vue",
            "options": {
                "htmlWhitespaceSensitivity": "css",
                "vueIndentScriptAndStyle": true,
                "tabWidth": 2
            }
        }
    ]
}
